<?xml version="1.0" encoding="utf-8"?>
<robot name="S0101_4MOTORS_DUAL_GRIPPER">
  
  <!-- BASE FIXE -->
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0.05" rpy="0 0 0" />
      <mass value="0.3" />
      <inertia ixx="0.0002" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0002" />
    </inertial>
    <visual>
      <origin xyz="0 0 0.05" rpy="0 0 0" />
      <geometry>
        <box size="0.08 0.08 0.10" />
      </geometry>
      <material name="gray">
        <color rgba="0.7 0.7 0.7 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.05" rpy="0 0 0" />
      <geometry>
        <box size="0.08 0.08 0.10" />
      </geometry>
    </collision>
  </link>

  <!-- BRAS FIXE -->
  <link name="arm_link">
    <inertial>
      <origin xyz="0 0 0.10" rpy="0 0 0" />
      <mass value="0.25" />
      <inertia ixx="0.0002" ixy="0" ixz="0" iyy="0.0003" iyz="0" izz="0.0001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0.10" rpy="0 0 0" />
      <geometry>
        <box size="0.04 0.04 0.20" />
      </geometry>
      <material name="blue">
        <color rgba="0.2 0.5 0.9 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.10" rpy="0 0 0" />
      <geometry>
        <box size="0.04 0.04 0.20" />
      </geometry>
    </collision>
  </link>

  <joint name="arm_fixed" type="fixed">
    <origin xyz="0 0 0.10" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="arm_link" />
  </joint>

  <!-- MOTEUR 4: WRIST PITCH -->
  <link name="wrist_pitch_link">
    <inertial>
      <origin xyz="0 0.03 0" rpy="0 0 0" />
      <mass value="0.08" />
      <inertia ixx="0.00003" ixy="0" ixz="0" iyy="0.00003" iyz="0" izz="0.00004" />
    </inertial>
    <visual>
      <origin xyz="0 0.03 0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.018" length="0.06" />
      </geometry>
      <material name="gray" />
    </visual>
    <collision>
      <origin xyz="0 0.03 0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.018" length="0.06" />
      </geometry>
    </collision>
  </link>

  <joint name="motor_4_wrist_pitch" type="revolute">
    <origin xyz="0 0 0.20" rpy="1.5708 0 0" />
    <parent link="arm_link" />
    <child link="wrist_pitch_link" />
    <axis xyz="1 0 0" />
    <limit lower="-1.67" upper="1.67" effort="1.0" velocity="20" />
    <dynamics damping="0.05" friction="0.05" />
  </joint>

  <!-- MOTEUR 5: WRIST ROLL -->
  <link name="wrist_roll_link">
    <inertial>
      <origin xyz="0 0.03 0" rpy="0 0 0" />
      <mass value="0.10" />
      <inertia ixx="0.00005" ixy="0" ixz="0" iyy="0.00004" iyz="0" izz="0.00006" />
    </inertial>
    <visual>
      <origin xyz="0 0.03 0" rpy="0 0 0" />
      <geometry>
        <box size="0.04 0.06 0.03" />
      </geometry>
      <material name="gray" />
    </visual>
    <collision>
      <origin xyz="0 0.03 0" rpy="0 0 0" />
      <geometry>
        <box size="0.04 0.06 0.03" />
      </geometry>
    </collision>
  </link>

  <joint name="motor_5_wrist_roll" type="revolute">
    <origin xyz="0 0.06 0" rpy="0 0 0" />
    <parent link="wrist_pitch_link" />
    <child link="wrist_roll_link" />
    <axis xyz="0 1 0" />
    <limit lower="-3.14159" upper="3.14159" effort="1.0" velocity="20" />
    <dynamics damping="0.05" friction="0.05" />
  </joint>

  <!-- GRIPPER 1: MOTEUR 6 (Fixed Jaw) -->
  <link name="gripper1_fixed">
    <inertial>
      <origin xyz="0.015 0.04 0" rpy="0 0 0" />
      <mass value="0.04" />
      <inertia ixx="0.00002" ixy="0" ixz="0" iyy="0.00002" iyz="0" izz="0.00003" />
    </inertial>
    <visual>
      <origin xyz="0.015 0.04 0" rpy="0 0 0" />
      <geometry>
        <box size="0.015 0.05 0.01" />
      </geometry>
      <material name="red">
        <color rgba="0.9 0.2 0.2 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0.015 0.04 0" rpy="0 0 0" />
      <geometry>
        <box size="0.015 0.05 0.01" />
      </geometry>
    </collision>
  </link>

  <joint name="gripper1_fixed_joint" type="fixed">
    <origin xyz="0.02 0.06 0" rpy="0 0 0" />
    <parent link="wrist_roll_link" />
    <child link="gripper1_fixed" />
  </joint>

  <!-- GRIPPER 1: MOTEUR 6 (Moving Jaw) -->
  <link name="gripper1_moving">
    <inertial>
      <origin xyz="-0.015 0.04 0" rpy="0 0 0" />
      <mass value="0.02" />
      <inertia ixx="0.000015" ixy="0" ixz="0" iyy="0.000005" iyz="0" izz="0.000012" />
    </inertial>
    <visual>
      <origin xyz="-0.015 0.04 0" rpy="0 0 0" />
      <geometry>
        <box size="0.015 0.05 0.01" />
      </geometry>
      <material name="red" />
    </visual>
    <collision>
      <origin xyz="-0.015 0.04 0" rpy="0 0 0" />
      <geometry>
        <box size="0.015 0.05 0.01" />
      </geometry>
    </collision>
  </link>

  <joint name="motor_6_gripper1" type="revolute">
    <origin xyz="-0.02 0.06 0" rpy="0 0 0" />
    <parent link="wrist_roll_link" />
    <child link="gripper1_moving" />
    <axis xyz="1 0 0" />
    <limit lower="-0.3" upper="0.8" effort="1.0" velocity="20" />
    <dynamics damping="0.02" friction="0.02" />
  </joint>

  <!-- GRIPPER 2: MOTEUR 2 (Fixed Jaw) -->
  <link name="gripper2_fixed">
    <inertial>
      <origin xyz="0 0.04 0.015" rpy="0 0 0" />
      <mass value="0.04" />
      <inertia ixx="0.00002" ixy="0" ixz="0" iyy="0.00002" iyz="0" izz="0.00003" />
    </inertial>
    <visual>
      <origin xyz="0 0.04 0.015" rpy="0 0 0" />
      <geometry>
        <box size="0.01 0.05 0.015" />
      </geometry>
      <material name="green">
        <color rgba="0.2 0.9 0.2 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0.04 0.015" rpy="0 0 0" />
      <geometry>
        <box size="0.01 0.05 0.015" />
      </geometry>
    </collision>
  </link>

  <joint name="gripper2_fixed_joint" type="fixed">
    <origin xyz="0 0.06 0.02" rpy="0 0 0" />
    <parent link="wrist_roll_link" />
    <child link="gripper2_fixed" />
  </joint>

  <!-- GRIPPER 2: MOTEUR 2 (Moving Jaw) -->
  <link name="gripper2_moving">
    <inertial>
      <origin xyz="0 0.04 -0.015" rpy="0 0 0" />
      <mass value="0.02" />
      <inertia ixx="0.000015" ixy="0" ixz="0" iyy="0.000005" iyz="0" izz="0.000012" />
    </inertial>
    <visual>
      <origin xyz="0 0.04 -0.015" rpy="0 0 0" />
      <geometry>
        <box size="0.01 0.05 0.015" />
      </geometry>
      <material name="green" />
    </visual>
    <collision>
      <origin xyz="0 0.04 -0.015" rpy="0 0 0" />
      <geometry>
        <box size="0.01 0.05 0.015" />
      </geometry>
    </collision>
  </link>

  <joint name="motor_2_gripper2" type="revolute">
    <origin xyz="0 0.06 -0.02" rpy="0 0 0" />
    <parent link="wrist_roll_link" />
    <child link="gripper2_moving" />
    <axis xyz="0 0 1" />
    <limit lower="-0.3" upper="0.8" effort="1.0" velocity="20" />
    <dynamics damping="0.02" friction="0.02" />
  </joint>

</robot>
